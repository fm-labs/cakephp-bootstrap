{
  "name": "fm-labs/cakephp-bootstrap",
  "description": "Bootstrap plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "cakephp/cakephp": "^4.0",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Bootstrap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\": "./vendor/cakephp/cakephp/src",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
      "Bootstrap\\Test\\": "tests"
    }
  }
}
